{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "loadBalancers_blogs_load_balancer_name": {
            "defaultValue": "blogs-load-balancer",
            "type": "String"
        },
        "publicIPAddresses_blogSiteIpAddress_v2_externalid": {
            "defaultValue": "/subscriptions/4ca27440-4053-487c-b024-37a2d898d6da/resourceGroups/blog-rg/providers/Microsoft.Network/publicIPAddresses/blogSiteIpAddress-v2",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Network/loadBalancers",
            "apiVersion": "2018-12-01",
            "name": "[parameters('loadBalancers_blogs_load_balancer_name')]",
            "location": "uksouth",
            "sku": {
                "name": "Standard",
                "tier": "Regional"
            },
            "properties": {
                "provisioningState": "Succeeded",
                "resourceGuid": "2280062f-69eb-4a8f-b606-cef2104698e0",
                "frontendIPConfigurations": [
                    {
                        "name": "frontEndLoadBlanacerPool",
                        "etag": "W/\"d13e5c82-c530-477d-9325-7ac18906ac6a\"",
                        "properties": {
                            "provisioningState": "Succeeded",
                            "privateIPAllocationMethod": "Dynamic",
                            "publicIPAddress": {
                                "id": "[parameters('publicIPAddresses_blogSiteIpAddress_v2_externalid')]"
                            }
                        }
                    }
                ],
                "backendAddressPools": [
                    {
                        "name": "BackEndLoadBalancerPool",
                        "etag": "W/\"d13e5c82-c530-477d-9325-7ac18906ac6a\"",
                        "properties": {
                            "provisioningState": "Succeeded"
                        }
                    }
                ],
                "loadBalancingRules": [
                    {
                        "name": "LoadBalancerRule",
                        "etag": "W/\"d13e5c82-c530-477d-9325-7ac18906ac6a\"",
                        "properties": {
                            "provisioningState": "Succeeded",
                            "frontendIPConfiguration": {
                                "id": "[concat(resourceId('Microsoft.Network/loadBalancers', parameters('loadBalancers_blogs_load_balancer_name')), '/frontendIPConfigurations/frontEndLoadBlanacerPool')]"
                            },
                            "frontendPort": 443,
                            "backendPort": 443,
                            "enableFloatingIP": false,
                            "idleTimeoutInMinutes": 4,
                            "protocol": "Tcp",
                            "enableTcpReset": false,
                            "loadDistribution": "Default",
                            "disableOutboundSnat": false,
                            "backendAddressPool": {
                                "id": "[concat(resourceId('Microsoft.Network/loadBalancers', parameters('loadBalancers_blogs_load_balancer_name')), '/backendAddressPools/BackEndLoadBalancerPool')]"
                            },
                            "probe": {
                                "id": "[concat(resourceId('Microsoft.Network/loadBalancers', parameters('loadBalancers_blogs_load_balancer_name')), '/probes/LoadBalancerHealthProbe')]"
                            }
                        }
                    }
                ],
                "probes": [
                    {
                        "name": "LoadBalancerHealthProbe",
                        "etag": "W/\"d13e5c82-c530-477d-9325-7ac18906ac6a\"",
                        "properties": {
                            "provisioningState": "Succeeded",
                            "protocol": "Https",
                            "port": 443,
                            "requestPath": "/readme.html",
                            "intervalInSeconds": 20,
                            "numberOfProbes": 2
                        }
                    }
                ],
                "inboundNatRules": [],
                "outboundRules": [],
                "inboundNatPools": []
            }
        }
    ]
}